{
  "schemaVersion": 1,
  "id": "bettershulkers",
  "version": "${version}",
  "name": "Better Shulkers",
  "description": "A server-side mod that adds many features to shulkers.",
  "authors": [
    "noramibu",
    "QPCrummer"
  ],
  "contact": {
    "homepage": ""
  },
  "license": "MIT",
  "icon": "assets/bettershulkers/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.noramibu.bettershulkers.fabric.BetterShulkersFabric"
    ]
